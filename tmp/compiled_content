        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#4b72a8', endColorstr='#395b7f',GradientType=0 );
        /* IE6-9 */ }
    #header_content #links a#about_link {
      border-bottom: solid 6px #930000; }
    #header_content #links a#projects_link {
      border-bottom: solid 6px #2caf82; }
    #header_content #links a#blog_link {
      border-bottom: solid 6px #d09412; }

